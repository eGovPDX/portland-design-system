{
  "name": "@cityofportland/types",
  "private": true,
  "version": "1.0.0-alpha.0",
  "description": "Shared TypeScript types for Portland Design System components",
  "author": {
    "name": "City of Portland, Oregon"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "~5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eGovPDX/portland-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/eGovPDX/portland-design-system/issues"
  },
  "homepage": "https://github.com/eGovPDX/portland-design-system#readme"
}
